 hidden;\r\n    display: flex;\r\n    position: relative;\r\n\r\n    .list{\r\n      line-height: 40px;\r\n      display: flex;\r\n      \r\n      li{\r\n        font-size: 14px;\r\n        padding-right: 20px;\r\n        width: 1000px;\r\n      }/* li */\r\n    }/* .list */\r\n    p{\r\n      color: #fff;\r\n      position: absolute;\r\n      right: 10px;\r\n      top: 5px;\r\n      font-weight: 900;\r\n      cursor: pointer;\r\n      z-index: 1;\r\n    }\r\n  }/* .rolling */\r\n}/* rolling_ex */\r\n\r\n@keyframes flowRolling {\r\n  0% {transform: translateX(0);}\r\n  100% {transform: translateX(-100%);}\r\n}\r\n/* 상단 flowrolling */\r\n.header_ex{\r\n  @include size(100%, auto);\r\n  position: relative;\r\n  border-bottom: 1px solid #fff;\r\n  box-sizing: border-box;\r\n  .header{\r\n    @include size(1300px, 80px);\r\n    margin: 0 auto;\r\n    .logo{\r\n      float: left;\r\n      margin-left: 45px;\r\n      font-family: $english;\r\n      color: #fff;\r\n      margin-top: 10px;\r\n      \r\n      :first-child{\r\n        font-size: 30px;\r\n      }\r\n      :last-child{\r\n        font-size: 12px;\r\n        text-align: center;\r\n      }\r\n    }/* logo */\r\n\r\n\r\n    .menu{\r\n      float: left;\r\n      margin-left: 45px;\r\n      font-family: $english;\r\n      color: white;\r\n      font-size: 20px;\r\n      .main_menu{\r\n        \r\n        .item{\r\n          float: left;\r\n          margin-top: 38px;\r\n          margin-right: 80px;\r\n          .item__name{\r\n\r\n          }\r\n          .item__contents{\r\n            width: 100%;\r\n            position: absolute;\r\n\r\n            margin-top: 16px;\r\n            display: none;\r\n            z-index: 999;\r\n            background-color: $main;\r\n            left: 0;\r\n\r\n            .contents__menu{\r\n              background-color: $main;\r\n              width: 800px;\r\n              height: 50px;\r\n              color: $sub;\r\n              position: relative;\r\n              margin: 0 auto;\r\n              \r\n              .inner{\r\n                display: flex;\r\n                width: 100%;\r\n               \r\n                li{\r\n                  float: left;\r\n                  margin-right: 30px;\r\n                  font-size: 14px;\r\n                  line-height: 50px;\r\n                }\r\n              }\r\n            }\r\n            .submenu01{\r\n              padding-left: 32px;\r\n            }\r\n            .submenu02{\r\n              padding-left: 265px;\r\n            }\r\n          }\r\n          .submenu03{\r\n            padding-left: 260px;\r\n          }\r\n          .submenu04{\r\n            padding-left: 375px;\r\n          }\r\n        }\r\n      }\r\n    }\r\n\r\n\r\n\r\n    .main_menu .item:hover .item__name{\r\n      color: $sub;\r\n      border-radius: 6px 6px 0 0;\r\n      display: inline-block;\r\n      position: relative;\r\n    }\r\n    .main_menu .item .item__name::after{\r\n      content: '';\r\n      width: 0%;\r\n      left: 50%;\r\n      height: 1px;\r\n      display: block;\r\n      position: absolute;\r\n      margin-top: 1px;\r\n      border-bottom: 1px solid #222;\r\n      transition:all 0s ease;\r\n    }\r\n    .main_menu .item:hover .item__name::after{\r\n      content: '';\r\n      width: 120%;\r\n      left: -5px;\r\n      height: 1px;\r\n      display: block;\r\n      position: absolute;\r\n      margin-top: 3px;\r\n      border-bottom: 1px solid #222;\r\n      transition:all .3s ease;\r\n    }\r\n    .main_menu .item:hover .item__contents {\r\n      display: block;\r\n    }\r\n\r\n\r\n\r\n    .gnb{\r\n      float: right;\r\n      margin-right: 20px;\r\n      margin-top: 32px;\r\n      .gi{\r\n        float: left;\r\n        cursor: pointer;\r\n        @include size(30px, 30px);\r\n        img{\r\n          width: 90%;\r\n        }\r\n        margin-right: 25px;\r\n      }\r\n    }/* gnb */\r\n  }/* header */\r\n}/* .header_ex */\r\n\r\n\r\n#bgvid {\r\n  z-index: -100;\r\n  overflow: hidden;\r\n  max-width: 100%;\r\n  width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n}\r\n/* bgi */\r\n\r\n"},"lineCount":null}},"error":null,"hash":"d2f3e3bc5007d6a97d13d88bd85699ec","cacheData":{"env":{}}}